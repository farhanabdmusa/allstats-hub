generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sign_up_type {
  id                                   Int    @id(map: "sign_up_type_pk") @default(autoincrement())
  name                                 String @unique(map: "sign_up_type_unique") @db.VarChar
  user_user_sign_up_typeTosign_up_type user[] @relation("user_sign_up_typeTosign_up_type")
}

model user {
  id                                           Int           @id(map: "user_pk") @default(autoincrement())
  uuid                                         String        @unique(map: "user_unique") @db.VarChar
  email                                        String?       @unique(map: "user_unique_email") @db.VarChar
  new_version                                  Boolean       @default(true)
  first_session                                DateTime      @default(now()) @db.Timestamp(6)
  last_session                                 DateTime      @default(now()) @db.Timestamp(6)
  manufacturer                                 String?       @db.VarChar
  device_model                                 String?       @db.VarChar
  os                                           String?       @db.VarChar
  os_version                                   String?       @db.VarChar
  is_virtual                                   Boolean       @default(false)
  last_ip                                      String?       @db.VarChar
  sign_up_type                                 Int?
  sign_up_type_user_sign_up_typeTosign_up_type sign_up_type? @relation("user_sign_up_typeTosign_up_type", fields: [sign_up_type], references: [id], onDelete: Restrict, map: "user_sign_up_type_fk")
}
